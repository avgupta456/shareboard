{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 next dev",
    "dev-windows": "set NODE_OPTIONS=--max_old_space_size=4096 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write app pages components",
    "db_types": "yarn run supabase gen types typescript --local > ./src/lib/db_types.tsx"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.3",
    "@mantine/dates": "^5.10.3",
    "@mantine/form": "^5.10.3",
    "@mantine/hooks": "^5.10.3",
    "@mantine/next": "^5.10.3",
    "@next/font": "13.1.6",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-ui-react": "^0.2.6",
    "@supabase/supabase-js": "^2.7.1",
    "dayjs": "^1.11.7",
    "next": "13.1.6",
    "pg": "^8.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-spinners": "^0.13.8",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "autoprefixer": "^10.4.13",
    "encoding": "^0.1.13",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "supabase": "^1.37.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.5"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "importOrder": [
      "^react",
      "^next",
      "^@",
      "^#",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
